{
    "name": "@rainbow-ic/sunbeam",
    "version": "0.0.13",
    "main": "./dist/cjs/index",
    "module": "./dist/esm/index",
    "unpkg": "./dist/esm/index",
    "files": [
        "src",
        "dist"
    ],
    "type": "module",
    "exports": {
        ".": {
            "require": {
                "types": "./dist/cjs/index.d.ts",
                "default": "./dist/cjs/index.js"
            },
            "import": {
                "types": "./dist/esm/index.d.ts",
                "default": "./dist/esm/index.js"
            }
        }
    },
    "scripts": {
        "prepare": "husky",
        "format": "prettier --write .",
        "test": "jest",
        "test:cov": "jest --coverage",
        "lint": "eslint .",
        "dev": "npx tsx src/index.ts",
        "clean": "tsc --build --clean && rm -rf dist",
        "build": "npm run clean  && tsc -b && tsc -p tsconfig.cjs.json"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    },
    "keywords": [],
    "author": "tranbaolong14@gmail.com",
    "license": "MIT",
    "description": "multiple ICP dexes lib",
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.12",
        "@types/source-map-support": "^0.5.10",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "globals": "^15.9.0",
        "husky": "^9.1.5",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^15.2.10",
        "prettier": "3.3.3",
        "prettier-plugin-rust": "^0.1.9",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "tslib": "^2.7.0",
        "tsx": "^4.19.0",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.4.0"
    },
    "dependencies": {
        "@alpaca-icp/token-adapter": "^1.0.21",
        "@babel/core": "^7.25.8",
        "@dfinity/agent": "^2.1.2",
        "@dfinity/candid": "^2.1.2",
        "@dfinity/ledger-icrc": "^2.6.0",
        "@dfinity/principal": "^2.1.2",
        "@slide-computer/signer": "^3.6.4",
        "@slide-computer/signer-agent": "^3.6.4",
        "babel-jest": "^29.7.0",
        "util": "^0.12.5"
    },
    "directories": {
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rainbow-ic/sunbeam.git"
    },
    "bugs": {
        "url": "https://github.com/rainbow-ic/sunbeam/issues"
    },
    "homepage": "https://github.com/rainbow-ic/sunbeam#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    }
